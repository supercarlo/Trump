                            { label: "Dec",  y: $graphmaker.CreationsPerYear($year, "12")  }
                        ]
                    },
                ]
            });
    chart.render();

    var chart = new CanvasJS.Chart("chartContainer2",
            {
                animationEnabled: true,
                title: {
                    text: "Types of products",
                },
